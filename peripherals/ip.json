{
    "GPIO" : {
        "moduleType": "GPIO",
        "source": "peripherals/gpio/gpio_main.sv",
        "verilogName": "ip_gpio_main",
        "addrBits": 4,
        "parameters":{
            "pins": {
                "paramName": "pins",
                "verilogName": "PINS",
                "paramType": "Number",
                "default": 8,
                "min": 1,
                "max": 32
            },
            "Verilog Instance Name": {
                "paramName": "Verilog Instance Name",
                "paramType": "Symbol"
            },
            "Base Address": {
                "paramName": "Base Address",
                "verilogName": "ADDR",
                "paramType": "Number"
            }
        },
        "interfaces": [
            {
                "type": "Simple_Mem_IF.COWI",
                "port": "Bus"
            },
            {
                "type": "inout",
                "port": "pins",
                "width": "$pins",
                "pinmap": [
                    [0, "A13", "3V3", "slow"],
                    [1, "K12", "3V3", "slow"]
                ]
            }
        ]
    },
    "SPI_MANAGER" : {
        "moduleType": "SPI_MANAGER",
