                "verilogName": "ADDR",
                "paramType": "Number"
            }
        },
        "interfaces": [
            {
                "type": "Simple_Mem_IF.COWI",
                "port": "Bus"
            },
            {
                "type": "output",
                "port": "pins_o",
                "width": "3",
                "pinmap": [
                    [0, "A13", "3V3", "slow"],
                    [1, "K12", "3V3", "slow"]
                ]
            },
            {
                "type": "input",
                "port": "pins_i",
                "width": "1",
                "pinmap": [
                    [0, "A13", "3V3", "slow"]
                ]
